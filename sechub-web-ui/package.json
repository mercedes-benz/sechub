    "vue": "^3.4.31",
    "vue-i18n": "^10.0.5",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@intlify/unplugin-vue-i18n": "^6.0.0",
