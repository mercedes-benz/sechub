# Start mode of WebUI.
# Possible values are: server, development
# The development mode just starts the container and waits
WEBUI_START_MODE="server"

# Spring profiles:
SPRING_PROFILES_ACTIVE="webui_localserver"

# SecHub Server
# For local server: https://localhost:8443
# For Docker network: https://sechub:8443/
SECHUB_SERVER_URL="https://sechub:8443/"

# User credentials for SecHub server
SECHUB_USERID="admin"
SECHUB_APITOKEN='myTop$ecret!'

# Enable Java Debugging
JAVA_ENABLE_DEBUG=false

# Keep the container alive after it crashed
KEEP_CONTAINER_ALIVE_AFTER_WEBUI_CRASHED=false
