{
  "name": "pds-scancode",
  "productIdentifier": "PDS_LICENSESCAN",
  "setup": {
    "baseURL": "https://pds-scancode:8444",
    "credentials": {
      "user": "env:SECHUB_PDS_SCANCODE_USERID",
      "password": "env:SECHUB_PDS_SCANCODE_PASSWORD"
    },
    "jobParameters": [
      {
        "key": "pds.config.product.timeout.minutes",
        "value": "600"
      },
      {
        "key": "pds.config.productidentifier",
        "value": "PDS_SCANCODE"
      },
      {
        "key": "pds.config.script.trustall.certificates.enabled",
        "value": "false"
      },
      {
        "key": "pds.config.supported.datatypes",
        "value": "SOURCE,BINARY"
      },
      {
        "key": "pds.config.use.sechub.storage",
        "value": "false"
      },
      {
        "key": "sechub.productexecutor.pds.timeout.minutes",
        "value": "605"
      },
      {
        "key": "sechub.productexecutor.pds.timetowait.nextcheck.milliseconds",
        "value": "10000"
      },
      {
        "key": "sechub.productexecutor.pds.trustall.certificates",
        "value": "true"
      },
      {
        "key": "scancode.processes",
        "value": "1"
      },
      {
        "key": "scancode.scan.diagnostics",
        "value": "true"
      },
      {
        "key": "scancode.timeout",
        "value": "36000"
      }
    ]
  },
  "executorVersion": 1,
  "enabled": true
}
